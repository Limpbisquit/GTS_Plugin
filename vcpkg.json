{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "gtsplugin",
  "version-string": "1.0.0",
  "description": "SKSE plugin for Size Matters",
  "features": {
    "plugin": {
      "description": "Build the plugin.",
      "dependencies": [
        "clib-util",
        "commonlibsse-ng",
        "glm",
        "toml11",
        "nlohmann-json",
        "magic-enum",
        "boost-pfr",
        "lz4"
      ]
    }
  },
  "default-features": ["plugin"],
  "overrides": [
    {
      "name": "toml11",
      "version": "4.3.0"
    },
    {
      "name": "magic-enum",
      "version": "0.9.7#1"
    },
    {
      "name": "glm",
      "version": "1.0.1#3"
    }
  ]
}
